{
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames",
    "prettier-plugin-merge"
  ],
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "endOfLine": "lf",
  "singleAttributePerLine": true,
  "printWidth": 70
}
